              <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d997.1326430753799!2d-48.48574707085058!3d-1.4557546999336415!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x92a48e842477ba95%3A0x3e49c20ca6bf770c!2sAv.%20Gentil%20Bitencourt%2C%20800%20-%20Batista%20Campos%2C%20Bel%C3%A9m%20-%20PA%2C%2066040-174!5e0!3m2!1spt-BR!2sbr!4v1576716562589!5m2!1spt-BR!2sbr" frameborder="0" style="border:0; width: 100%; height: 312px;" allowfullscreen></iframe>

            </div>
          </div>

          <div class="col-lg-6">
            <div class="row">

            </div>

            <div class="form">
              <form action="" method="post" role="form" class="contactForm">
                <div class="form-row">
                  <div class="form-group col-lg-6">
                    <input type="text" name="name" class="form-control" id="name" placeholder="Seu nome" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validation"></div>
                  </div>
                  <div class="form-group col-lg-6">
                    <input type="email" class="form-control" name="email" id="email" placeholder="Seu email" data-rule="email" data-msg="Please enter a valid email" />
                    <div class="validation"></div>
                  </div>
                </div>
                <div class="form-group">
                  <input type="text" class="form-control" name="subject" id="subject" placeholder="Assunto" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                  <div class="validation"></div>
                </div>
                <div class="form-group">
                  <textarea class="form-control" name="Mensagem" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="Mensagem"></textarea>
                  <div class="validation"></div>
                </div>
                <div class="text-center"><button type="submit" title="Send Message">Enviar</button></div>
              </form>
            </div>
          </div>

        </div>

      </div>
    </section>

